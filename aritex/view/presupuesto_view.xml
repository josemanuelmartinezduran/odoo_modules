<?xml version="1.0" encoding="utf-8"?>
<openerp>
	<data>
		<!-- Modificaciones a la vista	-->
		<record id="jmd_preupuesto_form" model="ir.ui.view">
			<field name="name">jmd.presupuesto.form</field>
			<field name="model">ae.presupuesto</field>
			<field name="inherit_id" ref="jmd_anima.presupuesto_form"/>
			<field name="arch" type="xml">
				<xpath expr="//button[@name='calcula_costos']" position="replace">
					<button name="calcula" type="object" string="Actualizar Costos" />
				</xpath>		
			</field>
		</record>	
	</data>
</openerp>